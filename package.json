{
  "name": "@kronos-integration/interceptor-stream",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/stream-transform-interceptor.mjs"
  },
  "keywords": [
    "kronos-interceptor"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/stream-transform-interceptor.mjs --section=API",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/stream-transform-interceptor.mjs"
  },
  "dependencies": {
    "@kronos-integration/interceptor": "^10.2.0"
  },
  "devDependencies": {
    "@kronos-integration/test-interceptor": "^7.0.8",
    "ava": "^3.14.0",
    "c8": "^7.3.5",
    "documentation": "github:arlac77/documentation",
    "semantic-release": "^17.3.0"
  },
  "engines": {
    "node": ">=14.15.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/interceptor-stream.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/interceptor-stream/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/interceptor-stream#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
